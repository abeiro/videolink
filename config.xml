<?xml version="1.0" encoding="utf-8"?>
<widget id="tk.smellof.videolink" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="WebIntent">
        <param name="android-package" value="com.borismus.webintent.WebIntent" />
    </feature>
    <name>VideoLink</name>
    <description> Extract and convert video links using external service
</description>
    <author email="augusto.beiro@gmail.com" href=""> Augusto Beiro
</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="19" />
    <icon src="www/img/logo.png" />
</widget>